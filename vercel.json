{
  "buildCommand": "prisma generate && pnpm run build",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 15,
      "memory": 1769
    }
  },
  "regions": ["fra1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate=300"
        }
      ]
    }
  ]
}